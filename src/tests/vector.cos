VEC: SETV VA00 VEC: MAKE 42 43
VEC: GETC VA00 VA01 VA02
DBG: ASRT VA01 EQ 42 AND VA02 EQ 43

SETV VA00 -179
SETV VA03 1

LOOP
* VA00 = angle
* VA01 = unit vector (VA00)
* VA42 = angle of VA01
* VA02 = difference of VA00 and VA42
    VEC: SETV VA01 VEC: UNIT VA00
    VEC: MULV VA01 5
    SETV VA02 VEC: ANGL VA01
    SETV VA42 VA02
    DOIF VA02 < 0
      ADDV VA02 360
    ENDI
    SUBV VA02 VA00
    ABSV VA02

    SETV VA10 VEC: MAGN VA01
    SUBV VA10 5
    ABSV VA10

    DOIF VA02 > 0.01 AND VA20 > 0.01 OR VA10 > 0.01
        DBG: OUTS "# nok@"
        DBG: OUTV VA00
        SETV VA03 0
        DBG: OUTS " got: "
        DBG: OUTV VA42
        DBG: OUTV VA20
        DBG: OUTS " v="
        DBG: OUTV VA01
        DBG: OUTS "\n"
        DBG: FAIL
    ENDI
    ADDV VA00 1
UNTL VA00 > 180

DBG: ASRT VA03 EQ 1
